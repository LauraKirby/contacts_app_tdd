<h1>My Contacts</h1>
<table>
<th>First name</th>
<th>Email</th>
<th># of appointments</th>

<% @contacts.each do |contact| %>
  <tr>
  <td><%= contact.first_name %></td>
  <td><%= contact.email %></td>
  <td><%= contact.appointments.size %></td>
  </tr>
<% end %>

</table>

<a href="/contacts/new">Add Contact</a>